{\n  "name": "block-de-notas-code-ia-mccomics",\n  "version": "1.0.0",\n  "description": "Editor revolucionario con múltiples búsquedas y reemplazos simultáneos, funciones avanzadas de IA y corrección automática de sintaxis",\n  "main": "main.js",\n  "author": "McComicServics",\n  "license": "MIT",\n  "homepage": "https://github.com/McComicServics/block-de-notas-code-ia-mccomics",\n  "repository": {\n    "type": "git",\n    "url": "https://github.com/McComicServics/block-de-notas-code-ia-mccomics.git"\n  },\n  "scripts": {\n    "start": "electron .",\n    "dev": "electron . --dev",\n    "build": "electron-builder",\n    "build-win": "electron-builder --win",\n    "build-mac": "electron-builder --mac",\n    "build-linux": "electron-builder --linux",\n    "pack": "electron-builder --dir",\n    "dist": "npm run build",\n    "test": "echo \"Error: no test specified\" && exit 1"\n  },\n  "devDependencies": {\n    "electron": "^27.0.0",\n    "electron-builder": "^24.6.4"\n  },\n  "dependencies": {\n    "@codemirror/autocomplete": "^6.10.2",\n    "@codemirror/commands": "^6.3.0",\n    "@codemirror/lang-javascript": "^6.2.1",\n    "@codemirror/lang-python": "^6.1.3",\n    "@codemirror/lang-html": "^6.4.6",\n    "@codemirror/lang-css": "^6.2.1",\n    "@codemirror/lang-json": "^6.0.1",\n    "@codemirror/language": "^6.9.1",\n    "@codemirror/legacy-modes": "^6.3.3",\n    "@codemirror/search": "^6.5.4",\n    "@codemirror/state": "^6.3.1",\n    "@codemirror/theme-one-dark": "^6.1.2",\n    "@codemirror/view": "^6.21.3",\n    "codemirror": "^6.0.1"\n  },\n  "build": {\n    "appId": "com.mccomicservics.block-de-notas-code-ia",\n    "productName": "Block de Notas Code IA McComics",\n    "directories": {\n      "output": "dist"\n    },\n    "files": [\n      "main.js",\n      "preload.js",\n      "src/**/*",\n      "node_modules/**/*"\n    ],\n    "win": {\n      "target": "nsis",\n      "icon": "assets/icon.ico"\n    },\n    "mac": {\n      "target": "dmg",\n      "icon": "assets/i